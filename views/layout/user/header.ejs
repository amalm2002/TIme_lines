<body>


    <!-- Search model -->
    <div class="search-model">
        <div class="h-100 d-flex align-items-center justify-content-center">
            <div class="search-close-switch">+</div>
            <form class="search-model-form">
                <input type="text" id="search-input" placeholder="Search here.....">
            </form>
        </div>
    </div>
    <!-- Search model end -->

    <!-- Header Section Begin -->
    <header class="header-section">
        <div class="container-fluid">
            <div class="inner-header">
                <div class="logo">
                    <a href="/"><img src="/userHome/img/logo.png" alt="Logo"></a>
                </div>
                <nav class="main-menu mobile-menu">
                    <ul>
                        <li><a class="active" href="/">Home</a></li>
                        <li><a href="#">Shop</a>
                            <ul class="sub-menu">
                                <li><a href="/productPage">Product Page</a></li>
                                <li><a href="/shoppingCartPage">Shopping Cart</a></li>
                            </ul>
                        </li>
                        <!-- <li><a href="/about">About</a></li>
                        <li><a href="/blog">Blog</a></li>
                        <li><a href="/contact">Contact</a></li> -->
                    </ul>
                </nav>

                <div class="header-right">
                    <img src="/userHome/img/icons/search.png" alt="Search" class="search-trigger">

                    <% if (typeof user !=='undefined' && user) { %>
                        <div class="user-icon">
                            <img src="/userHome/img/icons/man.png" alt="User Icon" id="user-icon">
                            <div class="dropdown-content">
                                <div class="user-info">
                                    <img src="<%= user.image ? '/images/products/' + user.image : '/images/default-user-icon.jpg' %>"
                                        alt="User Icon" class="user-img">
                                    <span>
                                        <%= user.name %>
                                    </span>
                                </div>
                                <div class="dropdown-links">
                                    <a href="/userProfile" class="dropdown-item"><i class="fa-solid fa-user"> My
                                            Profile</i></a>
                                    <a href="/logout" class="dropdown-item"><i class="fa-solid fa-right-from-bracket">
                                             Logout</i></a>
                                </div>
                            </div>
                        </div>
                        <% } else { %>
                            <div class="user-access">
                                <a href="/register">Register</a>
                                <a href="/register" class="in">Sign In</a>
                            </div>
                            <% } %>

                                <a href="#">
                                    <img src="/userHome/img/icons/bag.png" alt="Cart">
                                    <span>2</span>
                                </a>
                </div>
            </div>
        </div>
    </header>

    <!-- Header Info Begin -->
    <div class="header-info">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-4">
                    <div class="header-item">
                        <img src="/userHome/img/icons/delivery.png" alt="">
                        <p>Free shipping on orders over $30 in USA</p>
                    </div>
                </div>
                <div class="col-md-4 text-left text-lg-center">
                    <div class="header-item">
                        <img src="/userHome/img/icons/voucher.png" alt="">
                        <p>20% Student Discount</p>
                    </div>
                </div>
                <div class="col-md-4 text-left text-xl-right">
                    <div class="header-item">
                        <img src="/userHome/img/icons/sales.png" alt="">
                        <p>30% off on dresses. Use code: 30OFF</p>
                    </div>
                </div>
            </div>
        </div>
    </div>


    
    

